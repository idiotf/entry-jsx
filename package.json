{
  "name": "entry-jsx",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepack": "bun build --outfile=dist/index.mjs --minify --external=react --external=react-dom src && bun build --outfile=dist/index.cjs --minify --external=react --external=react-dom --format=cjs src && bun dts-bundle-generator --no-banner -o dist/index.d.ts src/index.ts"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.44.0"
  }
}
