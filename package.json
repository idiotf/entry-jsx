{
  "name": "entry-jsx",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.tsx",
      "deno": "./src/index.tsx",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepack": "bun build --outfile=dist/index.mjs --minify --packages=external src && bun build --outfile=dist/index.cjs --minify --packages=external --format=cjs src && dts-bundle-generator --no-banner -o dist/index.d.ts src/index.ts"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.43.0"
  }
}
